<head>

	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
	<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" />

	<link href="../Studtable/style.bundled1cf.css" rel="stylesheet" type="text/css" />

</head>

<body>
	<div style="margin-top: -350px;" class="d-flex flex-column flex-root">

		<div class="d-flex flex-row flex-column-fluid page">

			<div class="d-flex flex-column flex-row-fluid" id="kt_wrapper">
				<center>
					<h1 style="margin-top: -90px;">STUDENTS DETAILS</h1>
				</center>
				<div class="content d-flex flex-column flex-column-fluid" id="kt_content">

					<div class="d-flex flex-column-fluid">
						<!--begin::Container-->

						<div class="container">


							<div class="card card-custom">
								<a style="" href="/tutor/addstudent"><button id="add"
										style="margin-top: -60px; float: right;" class="btn btn-success">Add
										Student</button></a>

								<div class="card-body">
									<!--begin: Datatable-->

									<table class="table table-bordered table-hover table-checkable " id="kt_datatable">
										<thead>

											<tr>
												<th>
													<center>Roll No</center>
												</th>
												<th>
													<center>Name</center>
												</th>
												<th>
													<center>Edit</center>
												</th>
												<th>
													<center>Delete</center>
												</th>
											</tr>
										</thead>
										<tbody>
                                        {{#each students}}
											<tr>
												<td>
													<center><a style="color: black;" href="/tutor/studetails">{{this.Rollno}}</a>
													</center>
												</td>
												<td>
													<center><a style="color: black;" href="/tutor/studetails">{{this.Name}}</a>
													</center>
												</td>
												<td>
													<center><a href="/tutor/editstud/{{this._id}}"><img
															src="../Template/icons/themify-icons/SVG/pencil-alt.svg"
															alt="" srcset=""></a></center>
												</td>
												<td>
													<center> <a href="/tutor/addstudent"><img
																src="../Studtable/trash-9-24.ico" alt="" srcset=""></a>
													</center>
												</td>
											</tr>
                                        {{/each}}
										</tbody>
									</table>
								</div>
							</div>
						</div>
					</div>
				</div>

			</div>
		</div>
	</div>

</body>