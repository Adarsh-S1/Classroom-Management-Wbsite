<body id="bg">

<div class="page-wraper">
        <div class="page-content bg-white">
<div class="page-banner ovbl-dark"
                style="background-image:url(../Studtemplate/assets/images/banner/banner3.jpg);">
                <div class="container">
                    <div class="page-banner-entry">
                        <h1 class="text-white">NOTES</h1>
                    </div>
                </div>
            </div>
	<div  class="d-flex flex-column flex-root">

		<div class="d-flex flex-row flex-column-fluid page">

			<div class="d-flex flex-column flex-row-fluid" id="kt_wrapper">

				<div class="content d-flex flex-column flex-column-fluid" id="kt_content">

					<div class="d-flex flex-column-fluid">
						<!--begin::Container-->

						<div class="container">



							<div style="margin-left: -20px;" class="card card-custom">
								<div style="margin-left: -10px;" class="card-body">
									<!--begin: Datatable-->
									<table  class="table table-bordered table-hover table-checkable " id="kt_datatable">
										<thead>

											<tr>
												<th>
													<center>Topic</center>
												</th>
												<th>
													<center>File</center>
												</th>
												<th>
													<center>Submit</center>
												</th>
											</tr>
										</thead>
										<tbody>
                                         {{#each assign}}
											<tr>
												<td>      
													<center>{{this.Topic.Topic}}</center>
												</td>
												<td>
													<center><a href="../Assignments/{{this._id}}.pdf ">
  <center><img style="height: 40px; width: auto;" src="../Notes/open-document.png" alt="W3Schools" width="104" height="142"><br><small style="color: rgb(17, 165, 17);">Click to download the file</small></center></center>
												</td>
												<td>
													<center> <a href="/assignments/{{this._id}}"><button class="btn btn-success">Submit</button></a>
													</center>
												</td>
												<td hidden nowrap="nowrap"></td>
											</tr>
                                      {{/each}}
										</tbody>
									</table>
								</div>
							</div>
						</div>
					</div>
				</div>

			</div>
		</div>
	</div>
    </div>
    </div>

	<script src="../Template/js/vendors.min.js"></script>
